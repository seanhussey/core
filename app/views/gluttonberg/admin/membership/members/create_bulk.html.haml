- @window_title = "Membership"
- @page_title = "#{link_to "Membership", admin_membership_root_path} > #{link_to("Members", admin_membership_members_path)} > CSV processing report"

.row-fluid
  = render :partial => "/gluttonberg/admin/membership/main/sidebar"
  .span9
    .row-fluid
      .page-header
        %h1
          = @page_title.html_safe
      .subnav
      = render :partial => "list" , :locals => { :list => @failed , :list_type => "failed"}

      = render :partial => "list" , :locals => { :list => @successfull , :list_type => "successful"}

      = render :partial => "list" , :locals => { :list => @updated , :list_type => "updated"}

